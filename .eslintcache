POST',\n                headers: {\n                    'Content-Type': 'application/json',\n                    'Accept': 'application/json'\n                },\n                body: JSON.stringify({\n                    user: {\n                        firstName: this.state.firstName,\n                        lastName: this.state.lastName,\n                        username: this.state.username,\n                        email: this.state.email,\n                        password: this.state.password\n                    }\n                })\n            })\n            .then(this.setState({ redirect: !this.state.redirect }))\n        } else {\n            this.setState({ passwordMatch: !this.state.passwordMatch })\n        }\n\n    }\n\n    render () {\n        return (\n        <div id=\"signup\">\n            <form id=\"signup-form\" onSubmit={this.handleSubmit}>\n                <label>First Name</label>\n                <input onChange={this.handleChange} value={this.state.firstName} type=\"text\" name=\"firstName\" placeholder=\"First Name\"/>\n    \n                <label>Last Name</label>\n                <input onChange={this.handleChange} value={this.state.lastName} type=\"text\" name=\"lastName\" placeholder=\"Last Name\"/>\n    \n                <label>Username</label>\n                <input onChange={this.handleChange} value={this.state.username} type=\"text\" name=\"username\" placeholder=\"Username\"/>\n\n                <label>Email</label>\n                <input onChange={this.handleChange} value={this.state.email} type=\"text\" name=\"email\" placeholder=\"example@example.com\" />\n    \n                <label>Password</label>\n                <input onChange={this.handleChange} value={this.state.password} type=\"password\" name=\"password\" placeholder=\"••••••••\" />\n                {\n                    this.state.passwordMatch \n                    ? \n                    null \n                    : \n                    <p id=\"password-match-error-message\">Passwords do not match</p>\n                }\n                <label>Verify Password</label>\n                <input onChange={this.handleChange} value={this.state.passwordVerification} type=\"password\" name=\"passwordVerification\" placeholder=\"••••••••\" />\n    \n                <input type=\"submit\" value=\"Sign Up\" id=\"sign-up-button\" />\n                {\n                    this.state.redirect\n                    ?\n                    <Redirect to='/home' />\n                    :\n                    null\n                }\n            </form>\n            {/* <img id=\"signup-background\" src=\"https://i.ibb.co/pzQJCwv/Pngtree-green-light-diamond-shaped-box-945539.jpg\" /> */}\n        </div>\n        )\n    }\n}\n","/Users/colterulrich/Flatiron/5mod/audatix/frontend/src/Components/Signin.js",[],"/Users/colterulrich/Flatiron/5mod/audatix/frontend/src/index.js",[],"/Users/colterulrich/Flatiron/5mod/audatix/frontend/src/Components/Home.js",["54","55"],"/Users/colterulrich/Flatiron/5mod/audatix/frontend/src/Components/Upload.js",[],"/Users/colterulrich/Flatiron/5mod/audatix/frontend/src/Containers/SoundCollection.js",[],"/Users/colterulrich/Flatiron/5mod/audatix/frontend/src/Containers/FindSoundsPage.js",[],"/Users/colterulrich/Flatiron/5mod/audatix/frontend/src/Components/SoundCard.js",[],{"ruleId":"56","severity":1,"message":"57","line":11,"column":8,"nodeType":"58","messageId":"59","endLine":11,"endColumn":23},{"ruleId":"60","severity":1,"message":"61","line":33,"column":17,"nodeType":"62","endLine":33,"endColumn":20},{"ruleId":"60","severity":1,"message":"61","line":34,"column":17,"nodeType":"62","endLine":34,"endColumn":20},{"ruleId":"60","severity":1,"message":"61","line":35,"column":17,"nodeType":"62","endLine":35,"endColumn":20},{"ruleId":"60","severity":1,"message":"61","line":36,"column":17,"nodeType":"62","endLine":36,"endColumn":20},{"ruleId":"56","severity":1,"message":"63","line":2,"column":10,"nodeType":"58","mes